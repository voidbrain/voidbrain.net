{
  "name": "voidbrain.net",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",

    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
